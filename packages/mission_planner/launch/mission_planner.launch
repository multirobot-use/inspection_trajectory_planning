<launch>
    <arg name="drone_id" default="1"/>
    <!-- Launch mission_planner_node -->
    <node pkg="mission_planner" type="mission_planner_node" name="mission_planner_ros" output="screen" ns="drone_$(arg drone_id)">
        <rosparam file="$(find mission_planner)/param/mission_planner.yml" command="load"/>
        <param name="drone_id" value="$(arg drone_id)"/>
    </node>
</launch>
